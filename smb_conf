[homes]
   comment = Home Directories
   browseable = yes
   read only = no
   create mask = 0700
   directory mask = 0700
   valid users = %S

[public]
   comment = Public Anonymous Access
   path = /var/samba/
   browesable = yes
   create mask = 0660
   directory mask = 0771
   writeable = yes
   guest ok = yes
